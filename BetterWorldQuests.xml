	 <Button name="BWQ_QuestTemplate" hidden="false" inherits="SecureFrameTemplate" virtual="true">
		<Size>
			<AbsDimension x="260" y="40"/>
		</Size>
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="trackedBorder" atlas="PetList-ButtonSelect" hidden="true"/>
				<FontString parentKey="title" inherits="GameFontNormalLeft" maxLines="1">
					<Size x="155" y="0"/>
					<Anchors>
						<Anchor point="TOPLEFT" x="58" y="-6"/>
					</Anchors>
					<Color r="1" g="0.82" b="0" a="1"/>
				</FontString>
				<FontString parentKey="extra" inherits="GameFontNormalLeft" maxLines="1">
					<Size x="95" y="0"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="58" y="6"/>
					</Anchors>
					<Color r="0.8" g="0.8" b="0.8"/>
				</FontString>
				<FontString parentKey="time" inherits="GameFontNormalLeft" maxLines="1">
					<Size x="60" y="0"/>
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.extra" relativePoint="RIGHT" x="6"/>
					</Anchors>
					<Color r="0.75" g="0.61" b="0"/>
				</FontString>
			</Layer>
		</Layers>
		
		<HighlightTexture atlas="PetList-ButtonHighlight">
			<Color r="1" g="1" b="1" a="1"/>
		</HighlightTexture>
		
		<Frames>
			<Frame parentKey="type" hidden="true">
				<Size x="28" y="28" />
				<Anchors>
					<Anchor point="TOPLEFT" x="1" y="-6"/>
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND" textureSubLevel="-1">
						<Texture parentKey="elite" atlas="worldquest-questmarker-dragon"/>
					</Layer>
					<Layer level="BACKGROUND">
						<Texture parentKey="bg">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="ARTWORK">
						<Texture parentKey="texture">
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
		
			<Frame parentKey="faction">
				<Size x="20" y="20" />
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.type" relativePoint="RIGHT" x="1"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="icon"/>
					</Layer>
				</Layers>
			</Frame>
		
			<Frame parentKey="reward" hidden="true">
				<Size x="28" y="28" />
				<Anchors>
					<Anchor point="RIGHT" x="-8"/>
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="icon" file="Interface\InventoryItems\WoWUnknownItem01"/>
					</Layer>
					<Layer level="ARTWORK">
						<FontString parentKey="amount" inherits="BWQ_NumberFontOutline" maxLines="1" justifyH="RIGHT">
							<Anchors>
								<Anchor point="BOTTOMRIGHT" x="0" y="1"/>
							</Anchors>
							<Color r="1" g="1" b="1"/>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		
		<Scripts>
			<OnClick>
				PlaySound("igMainMenuOptionCheckBoxOn");
				if not self.questId or self.questId== -1 then return end
				if IsShiftKeyDown() then
					if IsWorldQuestHardWatched(self.questId) or (IsWorldQuestWatched(self.questId) and GetSuperTrackedQuestID() == self.questId) then
						BonusObjectiveTracker_UntrackWorldQuest(self.questId);
					else
						BonusObjectiveTracker_TrackWorldQuest(self.questId, true);
					end
				else
					if IsWorldQuestHardWatched(self.questId) then
						SetSuperTrackedQuestID(self.questId);
					else
						BonusObjectiveTracker_TrackWorldQuest(self.questId);
					end
				end
				
				SetMapByID(self.zoneId or 1007);
			</OnClick>
			<OnLoad>
				self:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnEnter>
				BWQ_Quest_OnEnter(self);
			</OnEnter>
			<OnLeave>
				WorldMapTooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>
	 
